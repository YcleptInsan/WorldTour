<h1>Add a new Band</h1>
<form action="/bands/new" method="post">
  <label for="band-name">Band name:</label>
  <input id="band-name" name="band-name" type="text">
  <select name="stylist-id">
    @foreach (var stylist in Model)
    {
      <option value="@venue.GetId()">@venue.GetName()</option>
    }
  </select>
  <button type="submit">Add new band</button>
</form>
<p><a href="/">Back home</a></p>
<p><a href="/bands">View all bands</a></p>
